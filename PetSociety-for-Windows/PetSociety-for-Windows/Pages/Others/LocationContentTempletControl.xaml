<UserControl x:Class="PetSociety_for_Windows.Pages.Others.LocationContentTempletControl"
 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="100" d:DesignWidth="200" d:Margin="0,0,0,0" Background="White" BorderBrush="Black">

    <StackPanel MinHeight="50" MinWidth="200">
        <StackPanel.Background>
            <SolidColorBrush Color="{StaticResource PhoneAccentColor}"/>
        </StackPanel.Background>

        <TextBlock Text="dsa" Name="title"/>

    </StackPanel>
</UserControl>